   "source": [
    "n, m = list(map(int, input().split()))\n",
    "l = list(map(int, input().split()))\n",
    "ans = 0\n",
    "\n",
    "for i in range(n):\n",
    "    for j in range(i+1, n):\n",
    "         for k in range(j+1, n):\n",
    "              s = l[i]+l[j]+l[k]\n",
    "              if ans < s and s <= m:\n",
    "                   ans = s\n",
    "\n",
    "\n",
    "print(ans)"
   ]
